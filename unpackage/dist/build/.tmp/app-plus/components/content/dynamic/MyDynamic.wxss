}
.my-dynamic .dyc-list .dyc-list-head-img {
    width: auto;
    margin-right: 20rpx;
}
.my-dynamic .dyc-list .dyc-list-head-img image {
      width: 90rpx;
      height: 90rpx;
      border-radius: 12rpx;
}
