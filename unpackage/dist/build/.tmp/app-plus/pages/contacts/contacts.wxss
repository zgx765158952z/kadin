.contacts {
  height: 100vh;
  font-size: 32rpx;
}
.contacts .contactstop .contacts-list, .contacts .contactscon .contacts-list {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding-left: 20rpx;
    background-color: #FFFFFF;
}
.contacts .contactstop .contacts-list .contacts-list-img, .contacts .contactscon .contacts-list .contacts-list-img {
      width: auto;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
}
.contacts .contactstop .contacts-list .contacts-list-img .image-friend, .contacts .contactscon .contacts-list .contacts-list-img .image-friend {
        font-family: 'iconfont';
        font-size: 60rpx;
}
.contacts .contactstop .contacts-list .contacts-list-img image, .contacts .contactscon .contacts-list .contacts-list-img image {
        width: 80rpx;
        height: 80rpx;
        border-radius: 8rpx;
}
.contacts .contactstop .contacts-list .contacts-list-title, .contacts .contactscon .contacts-list .contacts-list-title {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
              flex: 1;
      font-size: 32rpx;
      margin-left: 30rpx;
      padding: 30rpx 0;
      border-bottom: 1rpx solid rgba(100, 100, 100, 0.1);
}
.contacts .contactstop .contacts-list .bordernone, .contacts .contactscon .contacts-list .bordernone {
      border: none;
}
.contacts .contactstop .contacts-list:active, .contacts .contactscon .contacts-list:active {
    background-color: #eee;
}
.contacts .contactscon .letter-item {
    background-color: #F7F7F7;
    padding: 10rpx 30rpx;
}
.contacts contacts-index {
    z-index: 10000;
}
.contacts def-mask {
    position: fixed;
    z-index: 10001;
}

