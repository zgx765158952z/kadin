.consentfriend {
  min-height: 100vh;
}
.consentfriend .consentfriend-def-list .my-iconfont {
  color: #55A532;
  font-size: 36rpx;
}
.consentfriend .consentfriend-def-list .consentfriend-def-list-center {
  color: #181818;
}
.consentfriend .consent-list {
  margin-top: 30rpx;
}
.consentfriend .consent-list .consent-item {
  position: relative;
  background-color: #FFFFFF;
}
.consentfriend .consent-list .consent-item image {
  width: 92rpx;
  height: 92rpx;
}
.consentfriend .consent-list .consent-item .consent-btn {
  font-size: 32rpx;
  color: #007AFF;
