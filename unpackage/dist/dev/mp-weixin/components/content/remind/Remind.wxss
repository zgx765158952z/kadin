.remind {
  color: #666666;
  height: 100vh;
  background-color: #F8F8F8;
  font-size: 32rpx;
}
.remind .remind-head, .remind .remind-msg {
    position: relative;
    color: #333;
    background-color: #FFFFFF;
}
.remind .remind-head .remind-head-son, .remind .remind-head .remind-msg-son, .remind .remind-msg .remind-head-son, .remind .remind-msg .remind-msg-son {
      padding: 20rpx 30rpx;
}
.remind .remind-msg {
    margin-top: 60rpx;
}
.remind .all-list {
    margin: 60rpx 0;
}
.remind .all-list .remind-list {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      padding: 15rpx 30rpx;
      box-sizing: border-box;
      background-color: #FFFFFF;
}
.remind .all-list .remind-list .remind-list-left {
        width: auto;
}
.remind .all-list .remind-list .remind-list-right {
        text-align: right;
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
}
.remind .all-list .remind-list .remind-list-right .remind-list-right-icon {
          color: #999;
}
.remind .all-list .remind-list2 {
      padding: 25rpx 30rpx !important;
}
.remind .all-list .first-remind-list {
      padding: 15rpx 0 15rpx 30rpx;
}
.remind .all-list .switch-list, .remind .all-list .time-val-list {
      border-bottom: 1rpx solid rgba(144, 144, 144, 0.1);
}

